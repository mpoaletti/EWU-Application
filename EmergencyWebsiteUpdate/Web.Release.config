<?xml version="1.0" encoding="utf-8"?>

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings>
		<add key="okta:RedirectUri" value="https://ewu.uwsuper.edu/authorization-code/callback" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
		<add key="okta:PostLogoutRedirectUri" value="https://ewu.uwsuper.edu/" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
		<add key="PostLoginRedirectUri" value="https://ewu.uwsuper.edu/UpdateWebsite" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
		<add key="TestUpload" value="false" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
	</appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>